    align-items: flex-start;
    margin-bottom: 1rem;
    flex-wrap: wrap;
    gap: 0.5rem;
  }

  .experience-title {
    font-size: 1.25rem;
    font-weight: 700;
    color: var(--text-primary);
    margin: 0;
  }

  .experience-company {
    font-size: 1rem;
    color: var(--text-secondary);
    margin: 0.25rem 0 0 0;
  }

  .experience-date {
    font-size: 0.875rem;
    color: var(--text-secondary);
    white-space: nowrap;
  }

