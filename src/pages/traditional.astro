<style>
  .about-content {
    max-width: 800px;
    margin: 0 auto;
  }

  .about-text {
    animation: fadeIn 0.8s ease-out;
  }

  .lead-paragraph {
    font-size: 1.25rem;
    line-height: 1.8;
    color: var(--text-primary);
    margin-bottom: 1.5rem;
  }

  .about-text p {
    font-size: 1.0625rem;
    line-height: 1.8;
    color: var(--text-secondary);
    margin-bottom: 1.5rem;
  }

  .about-text strong {
    color: var(--primary-color);
    font-weight: 600;
  }

  .experience-grid {
    display: flex;
    flex-direction: column;
    gap: 1.5rem;
    max-width: 900px;
    margin: 0 auto;
  }

  .experience-card {
    background: rgba(255, 255, 255, 0.03);
    border: 1px solid rgba(255, 255, 255, 0.1);
    border-radius: 16px;
    padding: 2rem;
